+    if(!fr_ctx)
+        return 0;
+
+    bufsz = min_t(size_t, bufsz, count);
+    buf = kmalloc(bufsz, GFP_ATOMIC);
+    if(buf == NULL)
+        return 0;
+
+    bufsz--;
+
+    res = scnprintf(buf, bufsz, "start:%d len:%d\n", fr_ctx->start_offset, fr_ctx->len);
+
+    read = simple_read_from_buffer(user_buf, count, ppos, buf, res);
